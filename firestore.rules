rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /productos/{productoId} {
      allow read, write: if request.auth != null;
    }
    match /miembros/{miembroId} {
      allow read, write: if request.auth != null;
    }
    match /clientes/{clienteId} {
      allow read, write: if request.auth != null;
    }
    match /ventas/{ventaId} {
      allow read, write: if request.auth != null;
    }
  }
}
